# Database configuration
VECTOR_DB_TYPE=falkordb  # Options: falkordb, qdrant
FALKORDB_HOST=localhost
FALKORDB_PORT=
FALKORDB_COLLECTION=memories

# Previously used Qdrant config (kept for backward compatibility)
# VECTOR_DB_TYPE=qdrant
# QDRANT_HOST=localhost
# QDRANT_PORT=7333
# QDRANT_COLLECTION=memories

# Embedding configuration
EMBEDDING_PROVIDER=litellm
EMBEDDING_MODEL=bedrock/cohere.embed-multilingual-v3
# EMBEDDING_API_KEY=your-api-key-here  # If needed

# LLM configuration
LLM_BACKEND=openai
LLM_MODEL=gpt-4o-mini
LLM_API_KEY=your-openai-api-key-here
# LLM_BASE_URL=your-base-url-here  # If using a custom endpoint

# AWS configuration (if using Bedrock)
# AWS_ACCESS_KEY_ID=your-aws-access-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret-key
# AWS_REGION_NAME=your-aws-region
